{
  "name": "ideamarket-frontend",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --fix --ext .js,.ts,.tsx --ignore-path .gitignore .",
    "check-types": "tsc",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|json|ts|tsx)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "validate": "npm-run-all --parallel check-types check-format lint build"
  },
  "dependencies": {
    "@ethersproject/contracts": "^5.0.8",
    "@ethersproject/providers": "^5.0.17",
    "@ethersproject/solidity": "^5.0.7",
    "@headlessui/react": "0.2.0",
    "@portis/web3": "^2.0.0-beta.59",
    "@tailwindcss/aspect-ratio": "0.2.0",
    "@tailwindcss/forms": "0.2.1",
    "@tailwindcss/typography": "0.3.1",
    "@types/chart.js": "^2.9.28",
    "@types/lodash": "^4.14.167",
    "@uniswap/sdk": "3.0.3",
    "@walletconnect/web3-provider": "^1.3.1",
    "airtable": "0.10.1",
    "autoprefixer": "10.0.2",
    "axios": "^0.21.0",
    "bignumber.js": "9.0.1",
    "chart.js": "^2.9.4",
    "chrome-aws-lambda": "8.0.2",
    "classnames": "2.2.6",
    "fortmatic": "^2.2.1",
    "graphql": "15.4.0",
    "graphql-request": "3.3.0",
    "immer": "8.0.0",
    "lodash": "^4.17.20",
    "luxon": "1.25.0",
    "next": "10.0.2",
    "next-seo": "4.23.0",
    "nprogress": "0.2.0",
    "numeral": "^2.0.6",
    "postcss": "8.1.7",
    "puppeteer-core": "8.0.0",
    "react": "17.0.1",
    "react-cookie-consent": "^6.2.2",
    "react-dom": "17.0.1",
    "react-hot-toast": "1.0.2",
    "react-query": "2.26.2",
    "react-select": "3.1.0",
    "sha3": "^2.1.3",
    "use-breakpoint": "1.1.6",
    "walletlink": "^2.0.2",
    "web3": "1.2.11",
    "zustand": "3.2.0"
  },
  "devDependencies": {
    "@types/classnames": "2.2.11",
    "@types/luxon": "1.25.0",
    "@types/node": "14.14.8",
    "@types/react": "16.9.56",
    "@types/react-select": "3.0.26",
    "@typescript-eslint/eslint-plugin": "4.8.1",
    "@typescript-eslint/parser": "4.8.1",
    "babel-eslint": "10.1.0",
    "babel-plugin-inline-react-svg": "1.1.2",
    "eslint": "7.13.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": ">=4",
    "lint-staged": ">=10.5.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.1.2",
    "tailwindcss": "2.0.1",
    "typescript": "4.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "CI=1 npm-run-all --parallel check-types && lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|ts|tsx)": [
      "yarn run lint"
    ],
    "*.+(js|json|ts|tsx)": [
      "yarn run format"
    ]
  }
}

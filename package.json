{
  "name": "ideamarket-frontend",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=12.x"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --fix --max-warnings=0 --ext .js,.ts,.tsx --ignore-path .gitignore .",
    "check-types": "tsc",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|json|ts|tsx)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "validate": "npm-run-all --parallel check-types check-format lint build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ensdomains/ensjs": "^2.0.1",
    "@ethersproject/contracts": "^5.2.0",
    "@ethersproject/providers": "^5.2.0",
    "@ethersproject/solidity": "^5.2.0",
    "@headlessui/react": "1.2.0",
    "@heroicons/react": "1.0.1",
    "@portis/web3": "^2.0.0-beta.59",
    "@sentry/nextjs": "^6.11.0",
    "@tailwindcss/aspect-ratio": "0.2.1",
    "@tailwindcss/forms": "0.3.2",
    "@tailwindcss/typography": "0.4.1",
    "@types/chart.js": "^2.9.28",
    "@types/lodash": "^4.14.170",
    "@uniswap/sdk": "3.0.3",
    "@web3-react/core": "^6.1.9",
    "@web3-react/fortmatic-connector": "^6.1.6",
    "@web3-react/injected-connector": "^6.0.7",
    "@web3-react/portis-connector": "^6.1.9",
    "@web3-react/walletconnect-connector": "^6.2.0",
    "@web3-react/walletlink-connector": "^6.2.0",
    "airtable": "0.11.1",
    "autoprefixer": "10.2.6",
    "axios": "^0.21.1",
    "bignumber.js": "9.0.1",
    "chart.js": "^2.9.4",
    "chrome-aws-lambda": "9.1.0",
    "classnames": "2.3.1",
    "copy-to-clipboard": "^3.3.1",
    "graphql": "15.5.0",
    "graphql-request": "3.4.0",
    "immer": "9.0.2",
    "lodash": "^4.17.21",
    "luxon": "1.27.0",
    "next": "^11.1.1",
    "next-pwa": "^5.2.24",
    "next-seo": "4.24.0",
    "next-themes": "^0.0.14",
    "nprogress": "0.2.0",
    "numeral": "^2.0.6",
    "postcss": "8.3.0",
    "puppeteer-core": "9.1.1",
